  "author": "Florian Strasser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flostrasser/flex-gallery/issues"
  },
  "homepage": "https://github.com/flostrasser/flex-gallery#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "sass": "^1.41.1"
  },
  "scripts": {
    "build": "npm run build-js && npm run build-sass",
